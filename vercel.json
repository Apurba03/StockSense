{
  "version": 2,
  "name": "StockSense",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Home/**",
      "use": "@vercel/static"
    },
    {
      "src": "contact/**",
      "use": "@vercel/static"
    },
    {
      "src": "news/**",
      "use": "@vercel/static"
    },
    {
      "src": "about/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/contact",
      "dest": "contact/index.html"
    },
    {
      "src": "/news",
      "dest": "news/index.html"
    },
    {
      "src": "/",
      "dest": "Home/index.html"
    },
    {
      "src": "/about",
      "dest": "about/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
